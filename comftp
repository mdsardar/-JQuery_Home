<!-- 
Macro Name	: auth
Description	: Macro for Security Check.
Input		: hasAuthority('<Function Name>')

 -->
<#macro auth expr>
 <#assign security=JspTaglibs["http://www.springframework.org/security/tags"] />
 <@security.authorize access=expr>
  <#nested>
 </@security.authorize>
</#macro>


<!-- 
Macro Name	: formLabelAlone
Description	: Macro to display the label/form question alone with * for Mandatory field. 
Constrain	: This doesn't add TD tag to the screen. Doesn't add any formating exception adding * to Mandatory fields.
Input		: questionMap - Questionaire Map 
			  fieldName - Questionaire FLD Name
		
-->
<#macro formLabelAlone questionMap fieldName>
	<#if questionMap[fieldName] ??>
		<#if  questionMap[fieldName].isMandatory ?? && questionMap[fieldName].isMandatory == "Y"><font color="red">*</font><#else>&nbsp;&nbsp;</#if> ${questionMap[fieldName].formLabel}
	</#if>
</#macro>

<!-- 
Macro Name	: formLabelTD
Description	:Macro to display the label/form question alone with * for Mandatory field along with TD Tag 
Inputs		: questionMap - Questionaire Map
			  fieldName: Questionaire FLD Name
 			  htmlsetting : htmlSettings that are applicable for the TD
-->
<#macro formLabelTD questionMap fieldName htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??><TD class="cellboldodd" ${htmlsetting}><@formLabelAlone fieldName/></TD>
		<#else><TD class="cellboldodd" ><@formLabelAlone fieldName/></TD></#if>
	</#if>
</#macro>

<!-- Macro to display the data for the question. This displays the data as readonly field with out TD
This can be used for source mapped fields.
Input fields for the macro: 
		txtName: name of the field in the data VO
-->
<#macro formDataAlone questionMap fieldName dataVo txtName htmlsetting="">
		<#assign dataval = getData(dataVo,fieldName) />
		<#if ( questionMap[fieldName].isMandatory ?? && questionMap[fieldName].isMandatory == "Y") && (dataval == "" || dataval == " ")>
			${htmlsetting} >${dataVo[txtName]}
		<#else>
			${htmlsetting} >${dataVo[txtName]}
		</#if>
</#macro>

<!-- 

<!-- Macro to display the data for the question. This displays the data as readonly field with TD
This can be used for source mapped fields.
Input fields for the macro: 
		txtName: name of the field in the data VO
		tdWidth: Width of the TD
		align: Horizontal Align for the TD 
-->
<#macro formDataTD questionMap fieldName dataVo txtName htmlsetting="">
		<#assign dataval = getData(dataVo,txtName) />
		<#if (  questionMap[fieldName].isMandatory ?? && questionMap[fieldName].isMandatory == "Y") && (dataval == "" || dataval == " ")>
			<TD class="cellboldMandBorder" style="border-bottom: 1px solid red;" ${htmlsetting} >${dataval}</TD>
		<#else>
			<TD class="cellboldodd"  ${htmlsetting} >${dataval}</TD>
		</#if>
</#macro>

<!-- 
Macro to display the data for the question. This displays the data in text field without TD.
This can be used for editable fields that uses textbox.
Input fields for the macro: 
		fieldName: Questionaire field name (used to check whether the field is mandatory or not)
		txtName: name of the field in the data VO
		tdWidth: Width of the TD
		align: Horizontal Align for the TD 
-->
<#macro formTextAlone questionMap fieldName dataVo voFieldName htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#assign dataval = getData(dataVo,voFieldName) />
		<@displayTextField questionMap fieldName voFieldName dataval htmlsetting/>
	</#if>
</#macro>

<#macro formTextAloneWOVO questionMap fieldName dataval voFieldName htmlsetting>
	<#if questionMap[fieldName] ??>
		<@displayTextField questionMap fieldName voFieldName dataval htmlsetting/>
	</#if>
</#macro>

<#macro formTextAloneWONameId questionMap fieldName ctrlNameId dataVo voFieldName htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#assign dataval = getData(dataVo, voFieldName) />
		<@displayTextField questionMap fieldName ctrlNameId dataval htmlsetting/>
	</#if>
</#macro>

<#macro formTextAloneNameIdWithOnchange questionMap fieldName ctrlNameId dataVo voFieldName onchange htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#assign dataval = getData(dataVo, voFieldName) />
		<@displayTextFieldWithOnchange questionMap fieldName ctrlNameId dataval onchange htmlsetting/>
	</#if>
</#macro>

<#macro formTextAloneNameIdWithChildVoOnchange questionMap fieldName ctrlNameId dataVo childVo voFieldName onchange htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if dataVo??>
			<#if dataVo[childVo] ??>
				<#local finalDataVo = dataVo[childVo] />
				<#assign dataval = getData(finalDataVo, voFieldName) />		
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<@displayTextFieldWithOnchange questionMap fieldName ctrlNameId dataval onchange htmlsetting/>
	</#if>
</#macro>

<#macro formTextAloneWONameIdWithChildVo questionMap fieldName ctrlNameId dataVo childVo voFieldName htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if dataVo??>
			<#if dataVo[childVo] ??>
				<#local finalDataVo = dataVo[childVo] />
				<#assign dataval = getData(finalDataVo, voFieldName) />		
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<@displayTextField questionMap fieldName ctrlNameId dataval htmlsetting/>
	</#if>
</#macro>

<#macro formTextFromChildVo questionMap fieldName baseVo childVo voFieldName htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if baseVo??>
			<#if childVo??>
				<#assign dataval = getData(childVo, voFieldName) />	
				<#if dataval ??>
					<#assign dataval = dataval />
				<#else>
					<#assign dataval = "" />
				</#if>
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<@displayTextField questionMap fieldName voFieldName dataval htmlsetting/>
	</#if>
</#macro>

<#macro formTextWONameIdFromChildVo questionMap fieldName ctrlNameId baseVo childVo voFieldName htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if baseVo??>
			<#if childVo ??>
				<#assign dataval = getData(childVo, voFieldName) />		
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<@displayTextField questionMap fieldName ctrlNameId dataval htmlsetting/>
	</#if>
</#macro>

