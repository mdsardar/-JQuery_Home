	</#if>
</#macro>

<#macro formTextArea questionMap fieldName dataVo txtName htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#assign dataval = getData(dataVo,txtName) />
		<@displayTextAreaField questionMap fieldName txtName dataval htmlsetting/>
	</#if>
</#macro>
<#macro formTextAreawithOnChange questionMap fieldName ctrlNameId dataVo txtName onchange htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#assign dataval = getData(dataVo,txtName) />
		<@displayTextAreaFieldWithOnChangeFun questionMap fieldName ctrlNameId dataval onchange htmlsetting/>
	</#if>
</#macro>

<#macro displayTextAreaFieldWithOnChangeFun questionMap fieldName ctrlNameId dataval onchangefun htmlsetting="">
	<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
	<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
	</#if>
	<#if questionMap[fieldName].isMandatory ?? && questionMap[fieldName].isMandatory == "Y">
		<#assign removeBorderWhenOnchange= 'removeBorderColor(this);'/>
		<#if onchangefun??> <#assign onChangeFunction = removeBorderWhenOnchange+onchangefun/><#else> <#assign onChangeFunction = removeBorderWhenOnchange /></#if>
	<#else>
		<#if onchangefun??> <#assign onChangeFunction = onchangefun/><#else> <#assign onChangeFunction = "" /></#if>
	</#if>
	<#if  questionMap[fieldName].isMandatory ?? && questionMap[fieldName].isMandatory == "Y">
		<#if (dataval) == "">
		  <textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #ff0000" onchange="${onChangeFunction}" rows="2" cols="50" >${dataval?html}</textarea>
			
		<#else>
			<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #cccccc" onchange="${onChangeFunction}" rows="2" cols="50" >${dataval?html}</textarea>
		</#if>
	<#else>
		<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #cccccc" rows="2" cols="50" }>${dataval?html}</textarea>
	</#if>
</#macro>

<#macro formTextAreaWOId questionMap fieldName dataVo txtName htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
		<#assign dataval = getData(dataVo,txtName) />
		
		<#if  questionMap[fieldName].isMandatory ?? &&  questionMap[fieldName].isMandatory == "Y">
			<#if (dataval) == "">
				<textarea name="${txtName}" title="${hoverMsg}" 
									style="border:1px solid #ff0000" onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')" ${txthtmlsetting}  >${dataval?html}</textarea>
			<#else>
				<textarea name="${txtName}" title="${hoverMsg}" ${txthtmlsetting} 
						    onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">${dataval?html}</textarea>
			</#if>
		<#else>
			<textarea name="${txtName}" id="${txtName}" title="${hoverMsg}" ${txthtmlsetting} >${dataval?html}</textarea>
		</#if>
	</#if>
</#macro>

<#macro formTextAreaWONameIdWithChildVo questionMap fieldName ctrlNameId dataVo childVo voFieldName htmlsetting>
	<#if questionMap[fieldName] ??>
		<#if dataVo??>
			<#if dataVo[childVo] ??>
				<#local finalDataVo = dataVo[childVo] />
				<#assign dataval = getData(finalDataVo, voFieldName) />		
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
		<#if  questionMap[fieldName].isMandatory ?? && questionMap[fieldName].isMandatory == "Y">
		<#if (dataval) == "">
		    <textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #ff0000" onchange='removeBorderColor(this);' rows="2" cols="50" >${dataval?html}</textarea>
			
		<#else>
			<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #cccccc" onchange='removeBorderColor(this);' rows="2" cols="50" >${dataval?html}</textarea>
		</#if>
	<#else>
		<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #cccccc" rows="2" cols="50" >${dataval?html}</textarea>
	</#if>
	
	</#if>
</#macro>



<#macro formDropDown questionMap fieldName dataVo fieldValue pckListData htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
		<#assign dataval = getData(dataVo,fieldValue) />
		<#if  questionMap[fieldName].isMandatory ?? &&  questionMap[fieldName].isMandatory == "Y" && (dataval) == "">
			<span title="${hoverMsg}"><select id="${fieldValue}" name="${fieldValue}" ${txthtmlsetting}  style="border:1px solid #ff0000" 
					onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		<#else>
			<span title="${hoverMsg}"><select id="${fieldValue}" name="${fieldValue}" ${txthtmlsetting}  onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		</#if>
		<#list pckListData as pickList>
		<#if pickList.code == dataval >
				<option value="${pickList.code}" selected >${pickList.pickListDesc}</option>
		<#else>
				<option value="${pickList.code}" >${pickList.pickListDesc}</option>
		</#if>
		</#list>
		</select></span>
	</#if>
</#macro>

<#macro formDropDownDesc questionMap fieldName dataVo fieldValue pckListData htmlsetting="">
	<#if questionMap[fieldName] ??>
		
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
		<#assign dataval = getData(dataVo,fieldValue) />
		<#if  questionMap[fieldName].isMandatory ?? &&  questionMap[fieldName].isMandatory == "Y" && (dataval) == "">
			<span title="${hoverMsg}"><select name="${fieldValue}" ${txthtmlsetting}  style="border:1px solid #ff0000" 
					onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
			
		<#else>
			<span title="${hoverMsg}"><select name="${fieldValue}" ${txthtmlsetting}  onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		</#if>
		<#list pckListData as pickList>
		<#if pickList.pickListDesc == dataval >
			<#if pickList.code == '' >
				<option value="" selected="selected" >${pickList.pickListDesc}</option>
			<#else>
				<option value="${pickList.pickListDesc}" selected="selected" >${pickList.pickListDesc}</option>
			</#if>
		<#else>
			<#if pickList.code == '' >
				<option value="" >${pickList.pickListDesc}</option>
			<#else>
				<option value="${pickList.pickListDesc}" >${pickList.pickListDesc}</option>
			</#if>
		</#if>
		</#list>
		</select></span>
	</#if>
</#macro>

<#macro formDropDownWithoutVO questionMap fieldName dataval fieldValue pckListData htmlsetting>
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = htmlsetting /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
		<#if  questionMap[fieldName].isMandatory ?? &&  questionMap[fieldName].isMandatory == "Y" && (dataval) == "">
			<span title="${hoverMsg}"><select name="${fieldValue}" ${txthtmlsetting}  style="border:1px solid #ff0000" 
					onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		<#else>
			<span title="${hoverMsg}"><select name="${fieldValue}" ${txthtmlsetting}  onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		</#if>
		<#list pckListData as pickList>
		<#if pickList.pickListDesc == dataval >
			<#if pickList.code == '' >
				<option value="" selected="selected" >${pickList.pickListDesc}</option>
			<#else>
				<option value="${pickList.pickListDesc}" selected="selected" >${pickList.pickListDesc}</option>
			</#if>
		<#else>
				<#if pickList.code == '' >
					<option value="" >${pickList.pickListDesc}</option>
				<#else>
					<option value="${pickList.pickListDesc}" >${pickList.pickListDesc}</option>
				</#if>
		</#if>
		</#list>
		</select></span>
	</#if>
</#macro>

<#macro formDropDownWONameId questionMap fieldName ctrlNameId dataVo fieldValue pckListData htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
		<#assign dataval = getData(dataVo,fieldValue) />
		<#if  questionMap[fieldName].isMandatory ?? &&  questionMap[fieldName].isMandatory == "Y" && (dataval) == "">
			<span title="${hoverMsg}"><select id="${ctrlNameId}" name="${ctrlNameId}" ${txthtmlsetting}  style="border:1px solid #ff0000" 
					onblur='removeBorderColor(this);'>
		<#else>
			<span title="${hoverMsg}"><select id="${ctrlNameId}" name="${ctrlNameId}" ${txthtmlsetting}  >
		</#if>
		<#list pckListData as pickList>
		<#if pickList.code == dataval >
				<option value="${pickList.code}" selected >${pickList.pickListDesc}</option>
		<#else>
				<option value="${pickList.code}" >${pickList.pickListDesc}</option>
		</#if>
		</#list>
		</select></span>
	</#if>
</#macro>

<#macro formDropDownWONameIdWithChildVo questionMap fieldName ctrlNameId dataVo childVo fieldValue pckListData htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if dataVo??>
			<#if dataVo[childVo] ??>
				<#local finalDataVo = dataVo[childVo] />
				<#assign dataval = getData(finalDataVo, fieldValue) />		
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<@displaySelectDropdown questionMap fieldName ctrlNameId dataval pckListData htmlsetting/>
	</#if>
</#macro>

<#macro formWithTextArea questionMap fieldName ctrlNameId arrayIndex dataval htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
		<#if  questionMap[fieldName].isMandatory ?? && questionMap[fieldName].isMandatory == "Y">
			<#if (dataval) == "">
				<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" 
									style="border:1px solid #ff0000" onblur='removeBorderColor(this);' ${txthtmlsetting} rows="3" cols="50" >${dataval?html}</textarea>
			<#else>
				<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #cccccc" onchange='removeBorderColor(this);' rows="3" cols="50">${dataval?html}</textarea>
			</#if>
		<#else>
			<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #cccccc" rows="3" cols="50">${dataval?html}</textarea>
		</#if>
	</#if>
</#macro>


<#macro formDropDownWONameIdWithChildVoEdd questionMap fieldName ctrlNameId baseVo listVo arrayIndex voFieldName htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
		<#if baseVo??>
			<#if baseVo[listVo]??>
				<#local dataVoArr = baseVo[listVo] />
				<#if dataVoArr[arrayIndex]??>
					<#local finalDataVo = dataVoArr[arrayIndex] />
					<#assign dataval = getData(finalDataVo, voFieldName) />		
				<#else>
					<#assign dataval = "" />
				</#if>
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<#if  questionMap[fieldName].isMandatory ?? && questionMap[fieldName].isMandatory == "Y">
			<#if (dataval) == "">
				<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" 
									style="border:1px solid #ff0000" onblur='removeBorderColor(this);' ${txthtmlsetting} rows="3" cols="50">${dataval?html}</textarea>
			<#else>
				<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #cccccc" onchange='removeBorderColor(this);' rows="3" cols="50">${dataval?html}</textarea>
			</#if>
		<#else>
			<textarea name="${ctrlNameId}" id="${ctrlNameId}" title="${hoverMsg}" ${txthtmlsetting} style="border:1px solid #cccccc" rows="3" cols="50">${dataval?html}</textarea>
		</#if>
	</#if>
</#macro>


<#macro formMultiSelectWONameIdList questionMap fieldName fieldControlId finalDataVo voFieldName pckListData htmlsetting>
	<#if questionMap[fieldName] ??>
				<#assign dataval = getData(finalDataVo, voFieldName) />	
				<#assign pckListDataVal = finalDataVo[pckListData] />	
		<@formMultiSelectMini questionMap fieldName dataval fieldControlId pckListDataVal htmlsetting/>
	</#if>
</#macro>
<#macro formMultiSelectComboBoxList questionMap fieldName fieldValue finalDataVo voFieldName pckListData vopickList modelId htmlsetting>
	<#if questionMap[fieldName] ??>
				 <#assign pckListSourceData = finalDataVo[pckListData] />
				 <#assign pckListTargetData = finalDataVo[vopickList] />
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
	<#assign dataval = getArrayData(finalDataVo,voFieldName) />
	
	  <div class='select_container'>
			<div class='panel'>
				<#if questionMap[fieldName].isMandatory == "Y" && !dataval?has_content>
				  <span title="${hoverMsg}"><select multiple="multiple" id="from_${fieldValue}" name="${fieldValue}" ${txthtmlsetting} 
								 class="comboboxselect">
				<#else>
					<span title="${hoverMsg}"><select multiple="multiple" id="from_${fieldValue}" name="${fieldValue}" ${txthtmlsetting} class="comboboxselect" >
				</#if>	
				<#if pckListSourceData?has_content>	
					<#list pckListSourceData as pickList>
						<#assign isSelected = dataval?seq_contains(pickList.code)>
						<option  value="${pickList.code}">${pickList.pickListDesc}</option>
					</#list>
				</#if>
			    </select></span>
			 </div> 
			 
			 <div class='panel' style="margin-top: 10%;">
			 	<input name="" type="button" class="bbutton" value=">>" onclick="moveall('from_${fieldValue}', 'to_${fieldValue}', 'false')"><div class='clear'></div><br />
		        <input name="" type="button"  class="bbutton" value=">" onclick="moveto('from_${fieldValue}', 'to_${fieldValue}')"><div class='clear'></div><br />
		        <input name="" type="button" class="bbutton" value="<" onclick="movefrom('to_${fieldValue}', 'from_${fieldValue}')"><div class='clear'></div><br />
		        <input name="" type="button" class="bbutton" value="<<" onclick="moveall('to_${fieldValue}', 'from_${fieldValue}', 'true')"><div class='clear'></div> 
		      </div>
		    
		   <div class='panel'>
			<#if questionMap[fieldName].isMandatory == "Y" && !dataval?has_content>
			  <span title="${hoverMsg}"><select multiple="multiple" id="to_${fieldValue}" name="${fieldValue}" ${txthtmlsetting} 
							 class="comboboxselect">
			<#else>
				<span title="${hoverMsg}"><select multiple="multiple" id="to_${fieldValue}" name="${fieldValue}" ${txthtmlsetting} class="comboboxselect">
			</#if>	
			<#if pckListTargetData?has_content>	
				<#list pckListTargetData as pickList>
					<option  value="${pickList.code}">${pickList.pickListDesc}</option>
				</#list>
			</#if>
		    </select></span>
		 </div> 
		 <br /> 
		 <div style="margin-top: 40%;">
		 	<table width="100%">
		 		<tr>
					<td align="right">
					<#if questionMap[fieldName].isMandatory??>
					<#assign mandFlag=questionMap[fieldName].isMandatory>
					</#if>
						<a href="#" class="bbutton" id="saveCmtBtn" onclick="saveMultiselection('to_${fieldValue}','${fieldValue}', '${modelId}','${mandFlag}');">Submit</a>
						<a href="#" class="bbutton" id="cancelCmtBtn" onclick="cancelMultiselection('${modelId}');">Cancel</a>
					</td>
				</tr>
		 </table> 
		 </div>
		                                 
	 </div> 
		    

	</#if>
</#macro>



<#macro formMultiSelectWONameIdWithChildVo questionMap fieldName dataVo childVo voFieldName pckListData htmlsetting>
	<#if questionMap[fieldName] ??>
		<#if dataVo??>
			<#if dataVo[childVo] ??>
				<#local finalDataVo = dataVo[childVo] />
				<#assign dataval = getData(finalDataVo, voFieldName) />	
				<#assign pckListDataVal = finalDataVo[pckListData] />
				<#assign dataval = getArrayData(finalDataVo,voFieldName) />	
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<@formMultiSelectMini questionMap fieldName dataval voFieldName pckListDataVal htmlsetting/>
	</#if>
</#macro>

<#macro formMultiSelectComboBoxWithChildVo questionMap fieldName dataVo childVo fieldValue pckListData vopickList modelId alrtMsg htmlsetting>
	<#if questionMap[fieldName] ??>
		<#if dataVo??>
			<#if dataVo[childVo] ??>
				<#local finalDataVo = dataVo[childVo] />
				 <#assign pckListSourceData = finalDataVo[pckListData] />
				 <#assign pckListTargetData = finalDataVo[vopickList] />
				 
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
		<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
		</#if>
	<#assign dataval = getArrayData(finalDataVo,fieldValue) />
	 <#assign alertMsg = getArrayData(finalDataVo,alrtMsg) />
	  <div class='select_container'>
			<div class='panel'>
				<#if questionMap[fieldName].isMandatory == "Y" && !dataval?has_content>
				 <span title="${hoverMsg}"> <select multiple="multiple" id="from_${fieldValue}" name="${fieldValue}" ${txthtmlsetting} 
								 class="comboboxselect">
				<#else>
					<span title="${hoverMsg}"><select multiple="multiple" id="from_${fieldValue}" name="${fieldValue}" ${txthtmlsetting} class="comboboxselect" >
				</#if>	
				<#if pckListSourceData?has_content>	
					<#list pckListSourceData as pickList>
						<#assign isSelected = dataval?seq_contains(pickList.code)>
						<option  value="${pickList.code}">${pickList.pickListDesc}</option>
					</#list>
				</#if>
			    </select></span>
			 </div> 
			 
			 <div class='panel' style="margin-top: 10%;">
		        <input name="" type="button"  class="bbutton" value=">" onclick="movetoForEddCountry('from_${fieldValue}', 'to_${fieldValue}','${alertMsg}')"><div class='clear'></div><br />
		        <input name="" type="button" class="bbutton" value="<" onclick="movefrom('to_${fieldValue}', 'from_${fieldValue}')"><div class='clear'></div><br />
		        <input name="" type="button" class="bbutton" value="<<" onclick="moveall('to_${fieldValue}', 'from_${fieldValue}', 'true')"><div class='clear'></div> 
		      </div>
		    
		   <div class='panel'>
			<#if questionMap[fieldName].isMandatory == "Y" && !dataval?has_content>
			  <span title="${hoverMsg}"><select multiple="multiple" id="to_${fieldValue}" name="${fieldValue}" ${txthtmlsetting} 
							 class="comboboxselect" >
			<#else>
				<span title="${hoverMsg}"><select multiple="multiple" id="to_${fieldValue}" name="${fieldValue}" ${txthtmlsetting} class="comboboxselect">
			</#if>	
			<#if pckListTargetData?has_content>	
				<#list pckListTargetData as pickList>
					<option  value="${pickList.code}">${pickList.pickListDesc}</option>
				</#list>
			</#if>
		    </select></span>
		 </div> 
		 <br /> 
		 <div style="margin-top: 40%;">
		 	<table width="100%">
		 		<tr>
					<td align="right">
						<a href="#" class="bbutton" id="saveCmtBtn" onclick="saveEddMultiselection('to_${fieldValue}','${fieldValue}', '${modelId}','${alertMsg}');">Submit</a>
						<a href="#" class="bbutton" id="cancelCmtBtn" onclick="cancelMultiselection('${modelId}');">Cancel</a>
					</td>
				</tr>
		 </table> 
		 </div>
		                                 
	 </div> 
		    

	</#if>
</#macro>

<#macro formMultiSelectMini questionMap fieldName dataval fieldValue pckListData htmlsetting="">
   <#if questionMap[fieldName] ??>
    <#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
	<#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
	</#if>
	   <#if  questionMap[fieldName].isMandatory ?? &&  questionMap[fieldName].isMandatory == "Y" && !dataval?has_content>
			<span title="${hoverMsg}"><select multiple="multiple" id="${fieldValue}" name="${fieldValue}" ${txthtmlsetting}  class="comboboxselect" style="height:100px;border:1px solid #ff0000" 
					onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		<#else>
			<span title="${hoverMsg}"><select multiple="multiple" id="${fieldValue}" name="${fieldValue}" ${txthtmlsetting} class="comboboxselect" style="height:100px;" onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		</#if>	
			
			<#list pckListData as pickList>
				<#assign isSelected = dataval?seq_contains(pickList.code)>
				<option  value="${pickList.code}">${pickList.pickListDesc}</option>
			</#list>
		</select></span>
	</#if>
</#macro>

<#macro formMultiSelect questionMap fieldName dataVo fieldValue pckListData htmlsetting="">
   <#if questionMap[fieldName] ??>
    <#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
    <#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
	</#if>
	<#assign dataval = getArrayData(dataVo,fieldValue) />
	   <#if  questionMap[fieldName].isMandatory ?? &&  questionMap[fieldName].isMandatory == "Y" && !dataval?has_content>
			<span title="${hoverMsg}"><select multiple="multiple" id="${fieldValue}" name="${fieldValue}" ${txthtmlsetting}  class="comboboxselect" style="border:1px solid #ff0000" 
					onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		<#else>
			<span title="${hoverMsg}"><select multiple="multiple" id="${fieldValue}" name="${fieldValue}" ${txthtmlsetting} class="comboboxselect" onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		</#if>	
			
			<#list pckListData as pickList>
				<#assign isSelected = dataval?seq_contains(pickList.code)>
				<option  value="${pickList.code}">${pickList.pickListDesc}</option>
			</#list>
		</select></span>
	</#if>
</#macro>

<#macro formMultiSelectValuesArr questionMap fieldName dataval fieldValue fieldId pckListData htmlsetting="">
   <#if questionMap[fieldName] ??>
    <#if htmlsetting??> <#assign txthtmlsetting = htmlsetting/><#else> <#assign txthtmlsetting = "" /></#if>
    <#if questionMap[fieldName].iconInfoDesc ??>
			<#assign hoverMsg= questionMap[fieldName].iconInfoDesc/>
		<#else>
			<#assign hoverMsg= questionMap[fieldName].formLabel/>
	</#if>
	   <#if  questionMap[fieldName].isMandatory ?? &&  questionMap[fieldName].isMandatory == "Y" && ( !(dataval ??) || dataval[0] == "" || !(dataval?has_content) ) >
	   		
			<span title="${hoverMsg}"><select multiple="multiple" name="${fieldValue}"  id="${fieldId}" ${txthtmlsetting}  class="comboboxselect" style="border:1px solid #ff0000;height:100px;" 
					onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		<#else>
			<span title="${hoverMsg}"><select multiple="multiple" name="${fieldValue}" id="${fieldId}" ${txthtmlsetting} class="comboboxselect" style="height:100px;" onblur="highlightMandatoryField(this , '${questionMap[fieldName].isMandatory}')">
		</#if>	
			
			<#list pckListData as pickList>
			
				<#assign isSelected = (dataval?seq_contains(pickList.code))?string>
				 <#if isSelected ?? && isSelected=="true">
					<option  value="${pickList.code}" selected="selected" >${pickList.pickListDesc}</option>
				 <#else>
				 	<option  value="${pickList.code}" >${pickList.pickListDesc}</option>
				 </#if>
			</#list>
		</select></span>
	</#if>
</#macro>

