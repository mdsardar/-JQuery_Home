<#macro formTextAreaWONameIdFromArrrayVo questionMap fieldName ctrlNameId baseVo listVo arrayIndex voFieldName isDisabled htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if baseVo??>
			<#if baseVo[listVo]??>
				<#local dataVoArr = baseVo[listVo] />
				<#if dataVoArr[arrayIndex]??>
