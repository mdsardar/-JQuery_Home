<#macro formSelectWONameIdFromMultiArrrayVo questionMap fieldName ctrlNameId baseVo childVo detailsVo listVo arrayIndex voFieldName pckListData htmlsetting="">
	<#if questionMap[fieldName] ??>
		<#if baseVo??>
			<#if baseVo[childVo]??>
				<#if baseVo[childVo][detailsVo]??>
				 	<#assign finalVo=baseVo[childVo][detailsVo]>
					<#if finalVo[listVo]??>
						<#local dataVoArr = finalVo[listVo] />
							<#if dataVoArr[arrayIndex]??>
								<#local finalDataVo = dataVoArr[arrayIndex] />
								<#assign dataval = getData(finalDataVo, voFieldName) />		
							<#else>
								<#assign dataval = "" />
							</#if>
						
					<#else>
						<#assign dataval = "" />
					</#if>
				<#else>
				<#assign dataval = "" />
			</#if>
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<@displaySelectDropdown questionMap fieldName ctrlNameId dataval pckListData htmlsetting/>
	</#if>
</#macro>
