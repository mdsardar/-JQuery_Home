<#macro formTextWONameIdFromMapVo questionMap fieldName ctrlNameId baseVo childVo detailsVo mapVo keyValue arrayIndex voFieldName htmlsetting>
	<#if questionMap[fieldName] ??>
		<#if baseVo??>
			<#if baseVo[childVo]??>
				<#if baseVo[childVo][detailsVo]??>
				 	<#assign finalVo=baseVo[childVo][detailsVo]>
						<#if finalVo[mapVo]??>
							<#local mapVoArr = finalVo[mapVo] />
								<#if mapVoArr[keyValue]??>
									<#local dataVoArr = mapVoArr[keyValue] />
										<#if dataVoArr[arrayIndex]??>
											<#local finalDataVo = dataVoArr[arrayIndex] />
											<#assign dataval = getData(finalDataVo, voFieldName) />		
										<#else>
											<#assign dataval = "" />
										</#if>
							<#else>
								<#assign dataval = "" />
							</#if>
					<#else>
						<#assign dataval = "" />
					</#if>
					
				<#else>
				<#assign dataval = "" />
			</#if>
			<#else>
				<#assign dataval = "" />
			</#if>
		<#else>
			<#assign dataval = "" />
		</#if>
		<@displayTextField questionMap fieldName ctrlNameId dataval htmlsetting/>
	</#if>
</#macro>

